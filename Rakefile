require 'rspec/core/rake_task'

namespace :build do
  task :all do
    puts 'build:all'
  end

  task :aws do
    puts 'build:aws'
  end

  task :gcp do
    puts 'build:gcp'
  end

  task :vsphere do
    puts 'build:vsphere'
  end

  task :azure do
    puts 'build:azure'
  end

  task :openstack do
    puts 'build:openstack'
  end
end
